<yas>
    <scene root="root" axis_length="5" />

    <views default="cam0">
        <perspective id="cam0" near="0.1" far="500" angle="23">
            <from x="60" y="60" z="60" />
            <to x="0" y="0" z="0" />
        </perspective>

        <ortho id="cam1" near="0.1" far="500" left="0" right="10" top="6" bottom="0">
            <from x="50" y="0" z="5" />
            <to x="5" y="0" z="0" />
        </ortho>
    </views>

    <ambient>
        <ambient r="0" g="0" b="0" a="1" />
        <background r="0.1" g="0.1" b="0.1" a="1" />
    </ambient>

    <lights>
        <omni id="light0" enabled="1" >
            <location x="16" y="20" z="10" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
        </omni>

        <omni id="light1" enabled="1" >
            <location x="-16" y="20" z="10" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
        </omni>

        <omni id="light2" enabled="1" >
            <location x="16" y="20" z="-10" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
        </omni>

        <omni id="light3" enabled="1" >
            <location x="-16" y="20" z="-10" w="1" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
        </omni>

        <spot id="light4" enabled="1" angle="23" exponent="1">
            <location x="0" y="20" z="0" w="10" />
            <target x="0" y="0" z="0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1" />
            <diffuse r="1" g="1" b="1" a="1" />
            <specular r="0.3" g="0.3" b="0.3" a="1" />
        </spot>
    </lights>

    <textures>
        <texture id="light-wood" file="scenes/images/light-wood.jpg" />
    </textures>

    <materials>
        <material id="root" shininess="500" >
            <specular r="0.3" g="0.3" b="0.3" a="1.0" />
            <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <emission r="0" g="0" b="0" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="boat-transf">
            <translate x="0" y="0" z="0" />
        </transformation>
    </transformations>

    <animations>
    </animations>

    <primitives>
        <primitive id="board">
            <board />
        </primitive>

        <primitive id="cube-face">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
            </transformation>

            <materials>
                <material id="root" />
            </materials>

            <texture id="none" length_s="1" length_t="1" />

            <children>
                <componentref id="board" />
                <componentref id="table" />
            </children>
        </component>

        <component id="board">
            <transformation>
                <translate x="0" y="12.7" z="0" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="none" length_s="1" length_t="1" />

            <children>
                <primitiveref id="board" />
            </children>
        </component>

        <component id="table">
            <transformation>
                <translate x="0" y="12" z="0" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="table-top" />
                <componentref id="table-leg-1" />
                <componentref id="table-leg-2" />
                <componentref id="table-leg-3" />
                <componentref id="table-leg-4" />
            </children>
        </component>

        <component id="table-top">
            <transformation>
                <scale x="34" y="1" z="24" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="cube" />
            </children>
        </component>

        <component id="table-leg-1">
            <transformation>
                <translate x="16" y="-6" z="10" />
                <scale x="1" y="12" z="1" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="cube" />
            </children>
        </component>

        <component id="table-leg-2">
            <transformation>
                <translate x="-16" y="-6" z="10" />
                <scale x="1" y="12" z="1" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="cube" />
            </children>
        </component>

        <component id="table-leg-3">
            <transformation>
                <translate x="16" y="-6" z="-10" />
                <scale x="1" y="12" z="1" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="cube" />
            </children>
        </component>

        <component id="table-leg-4">
            <transformation>
                <translate x="-16" y="-6" z="-10" />
                <scale x="1" y="12" z="1" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="light-wood" length_s="1" length_t="1" />

            <children>
                <componentref id="cube" />
            </children>
        </component>

        <component id="cube">
            <transformation>
            </transformation>

            <materials>
                <material id="root" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <componentref id="face1" />
                <componentref id="face2" />
                <componentref id="face3" />
                <componentref id="face4" />
                <componentref id="face5" />
                <componentref id="face6" />
            </children>
        </component>

        <component id="face1">
            <transformation>
                <translate x="0" y="0" z="0.5" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>

        <component id="face2">
            <transformation>
                <translate x="-0.5" y="0" z="0" />
                <rotate axis="y" angle="-90" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>

        <component id="face3">
            <transformation>
                <translate x="0" y="0" z="-0.5" />
                <rotate axis="y" angle="180" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>

        <component id="face4">
            <transformation>
                <translate x="0.5" y="0" z="0" />
                <rotate axis="y" angle="90" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>

        <component id="face5">
            <transformation>
                <translate x="0" y="-0.5" z="0" />
                <rotate axis="x" angle="90" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>

        <component id="face6">
            <transformation>
                <translate x="0" y="0.5" z="0" />
                <rotate axis="x" angle="-90" />
            </transformation>

            <materials>
                <material id="inherit" />
            </materials>

            <texture id="inherit" length_s="1" length_t="1" />

            <children>
                <primitiveref id="cube-face" />
            </children>
        </component>
    </components>
</yas>
